-- [[ V6.0 PREMIUM UTILITY EXPANSION ]]

function StudiosLib:CreateBezierCurve(p0, p1, p2, t)
    -- Quadratic Bezier Curve formula for smooth UI motions
    -- Used for advanced notification trails or menu transitions
    return (1 - t)^2 * p0 + 2 * (1 - t) * t * p1 + t^2 * p2
end

function StudiosLib:AnimateText(label, text, speed)
    -- V6.0 Typewriter effect with dynamic delay per character
    label.Text = ""
    for i = 1, #text do
        label.Text = text:sub(1, i)
        task.wait(speed or 0.05)
    end
end

