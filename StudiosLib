function StudiosLib:PerformanceCheck()
    -- Heuristic check to adjust render quality on low-end devices
    local fps = 1 / task.wait()
    if fps < 30 then
        InternalLog("Modo de baixa performance ativado (Desabilitando Glow)")
        -- Logic to disable MouseGlow if needed
    end
end

